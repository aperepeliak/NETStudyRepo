<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <!-- PROVIDER -->
    <add key="provider" value="System.Data.SqlClient" />

    <!-- CONNECTION STRING -->
    <!--<add key="connectionString" value="Data Source=(local)\SQLEXPRESS;
         Initial Catalog=AutoLot; Integrated Security=True" />-->
  </appSettings>

  <connectionStrings>

    <add connectionString="Data Source=(local)\SQLEXPRESS;
         Initial Catalog=AutoLot;Integrated Security=SSPI" name="AutoLotSqlProvider" />

    <add connectionString="Provider=SQLOLEDB;Data Source=(local)\SQLEXPRESS;
         Initial Catalog=AutoLot;Integrated Security=SSPI" name="AutoLotOleDbProvider" />

  </connectionStrings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
</configuration>